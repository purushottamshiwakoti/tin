{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "arcanedev/laravel-settings": "^9.0",
        "barryvdh/laravel-dompdf": "^1.0.0",
        "cviebrock/eloquent-sluggable": "^9.0.0",
        "doctrine/dbal": "^3.3.2",
        "guzzlehttp/guzzle": "^7.4.1",
        "intervention/image": "^2.7.1",
        "jrean/laravel-user-verification": "10.0.0",
        "laravel/framework": "^9.0",
        "laravel/helpers": "^1.5",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7.0",
        "laravel/ui": "^3.4",
        "laravolt/avatar": "^4.1",
        "league/glide-laravel": "^1.0",
        "league/omnipay": "^3.2.1",
        "lucidarch/lucid": "^1.0",
        "maatwebsite/excel": "^3.1",
        "omnipay/paypal": "^3.0",
        "spatie/laravel-image-optimizer": "^1.6.2",
        "spatie/laravel-permission": "^5.5.0",
        "spatie/laravel-sitemap": "^6.2",
        "yajra/laravel-datatables-oracle": "~9.19.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6.7",
        "filp/whoops": "^2.14.5",
        "fzaninotto/faker": "^1.9.2",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "~9.5.13",
        "symfony/css-selector": "~6.0.3",
        "symfony/dom-crawler": "~6.0.3"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"            
        },
        "files":[
            "app/Services/Backend/Helpers/BackendHelpers.php",
            "app/Helpers/AppHelpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
